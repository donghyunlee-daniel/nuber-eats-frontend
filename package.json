{
  "name": "nuber-eats-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@graphql-codegen/cli": "^5.0.3",
    "@parcel/watcher": "^2.4.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.113",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "graphql": "^16.9.0",
    "mock-apollo-client": "^1.3.1",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "tailwind:build": "npx tailwindcss-cli@latest build -i src/styles/tailwind.css -o src/styles/styles.css",
    "start": "npm run compile & npm run tailwind:build & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@testing-library/react": "^16.0.1",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/components/**/*.tsx",
      "./src/pages/**/*.tsx",
      "./src/routers/**/*.tsx"
    ]
  }
}
